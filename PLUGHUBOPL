    end
})    

-- Toggle para Auto Compass
local ToggleQuest = MainTab:CreateToggle({
    Name = "Go To Compass (Need Click to Collect Box)",
    CurrentValue = false,
    Flag = "ToggleAutoCompass",
    Callback = function()
        local player = game.Players.LocalPlayer
        local backpack = player.Backpack
        local character = player.Character

        -- Encontrar todos os itens "Compass" no inventário
        local compasses = {}
        for _, item in ipairs(backpack:GetChildren()) do
            if item.Name == "Compass" then
                table.insert(compasses, item)
            end
        end

        -- Iterar por cada "Compass" encontrado
        for _, Compass in ipairs(compasses) do
            -- Equipar o Compass
            Compass.Parent = character
            task.wait(1.5) -- Tempo aumentado para garantir que o item esteja devidamente equipado

            -- Verificar se o compass está equipado no personagem
            local equippedCompass = character:FindFirstChild("Compass")
            if equippedCompass then
                -- Posição do Compass
                local compassPosition = equippedCompass:WaitForChild("Poser").Value
                local char = character:FindFirstChild("HumanoidRootPart")
                
                if char then
                    -- Teletransporte do personagem
                    char.CFrame = CFrame.new(compassPosition)
                    task.wait(0.5) -- Aguardar um pouco para garantir que o teleporte foi concluído
                end

                -- Simular autoclique com o Compass
                local activateEvent = equippedCompass:FindFirstChild("Activate")
                local clickDetector = equippedCompass:FindFirstChild("ClickDetector")
                local startTime = tick() -- Marca o tempo de início
                local duration = 6 -- Duração em segundos para o autoclique

                while tick() - startTime < duration do
                    if activateEvent then
                        activateEvent:FireServer()
                    elseif clickDetector then
                        clickDetector:Click()
                    end
                    task.wait(0.1) -- Intervalo entre cliques (ajuste conforme necessário)
                end
            else
                warn("Falha ao equipar o Compass.")
            end
        end
    end
})


