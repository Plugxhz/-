    Name = "Jump Power",
    CurrentValue = "",
    PlaceholderText = "Enter Jump Power",
    RemoveTextAfterFocusLost = false,
    Callback = function(txt)
        local player = game.Players.LocalPlayer
        if player and player.Character and player.Character:FindFirstChild("Humanoid") then
            player.Character.Humanoid.JumpPower = tonumber(txt) or player.Character.Humanoid.JumpPower
        end
    end
})

local Input = MiscTab:CreateInput({
    Name = "Hip Height | Default 0 |",
    CurrentValue = "",
    PlaceholderText = "Enter Hip Height",
    RemoveTextAfterFocusLost = false,
    Callback = function(txt)
        local player = game.Players.LocalPlayer
        if player and player.Character and player.Character:FindFirstChild("Humanoid") then
            player.Character.Humanoid.HipHeight = tonumber(txt) or player.Character.Humanoid.HipHeight
        end
    end
})

local Input = MiscTab:CreateInput({
    Name = "Walk Speed", -- Nome atualizado para refletir o novo propósito
    CurrentValue = "",
    PlaceholderText = "Enter Walk Speed", -- Placeholder atualizado
    RemoveTextAfterFocusLost = false,
    Callback = function(txt)
        local player = game.Players.LocalPlayer
        if player and player.Character and player.Character:FindFirstChild("Humanoid") then
            player.Character.Humanoid.WalkSpeed = tonumber(txt) or player.Character.Humanoid.WalkSpeed
        end
