    local Event = game:GetService("Workspace").UserData["User_"..game.Players.LocalPlayer.UserId].ChallengesRemote
    Event:FireServer(A_1,A_2)
wait(.8)
local A_1 = "Claim"
local A_2 = "Challenge12"
    local Event = game:GetService("Workspace").UserData["User_"..game.Players.LocalPlayer.UserId].ChallengesRemote
    Event:FireServer(A_1,A_2)
wait(.8)
local A_1 = "Claim"
local A_2 = "Challenge13"
    local Event = game:GetService("Workspace").UserData["User_"..game.Players.LocalPlayer.UserId].ChallengesRemote
    Event:FireServer(A_1,A_2)
wait(.8)
local A_1 = "Claim"
local A_2 = "Challenge14"
    local Event = game:GetService("Workspace").UserData["User_"..game.Players.LocalPlayer.UserId].ChallengesRemote
    Event:FireServer(A_1,A_2)
wait(.8)
            end
        end)
    end
end)

local MiscSection = SkillTab:CreateSection("Spam Skill")

-- Variáveis globais para controle de spams
local skillmax = false
_G.quake1 = false
_G.lightbeam = false
_G.flare1 = false
_G.flare2 = false
_G.darkstar = false
local remotes = {}

local Toggle = SkillTab:CreateToggle({
    Name = "Skill Charge Max",
    CurrentValue = false,
    Callback = function(SKM)
        skillmax = SKM
    end    
})

local mta = getrawmetatable(game)
local namecall = mta.__namecall

-- Verifica se setreadonly ou make_writable estão disponíveis antes de usá-las
if setreadonly then
    setreadonly(mta, false)
elseif make_writable then
    make_writable(mta, false)
else
    warn("setreadonly ou make_writable não estão disponíveis no ambiente atual")
end

mta.__namecall = newcclosure(function(self, ...)
    local args = {...}
    local method = getnamecallmethod()

    if method == 'FireServer' or method == "InvokeServer" then
        if self.Name == 'Drown' and _G.nowaterdamage then
            return nil
        end
    end
    
    return namecall(self, ...)    
end)


mta.__namecall = newcclosure(function(self, ...)
    local args = {...}
    local method = getnamecallmethod()

    if method == 'FireServer' or method == "InvokeServer" then
        if self.Name == 'Drown' and _G.nowaterdamage then
            return nil
        end
    end
    
    return namecall(self, ...)    
end)

-- Função para serializar a tabela remotes
local function serializeTable(val)
    local serialized = "{"
    for k, v in pairs(val) do
        if type(v) == "table" then
            serialized = serialized .. serializeTable(v)
        else
            serialized = serialized .. tostring(v) .. ", "
        end
    end
    return serialized .. "}"
end

-- Quake Spam
local Toggle = SkillTab:CreateToggle({
    Name = "Quake Spam | Quake Wave |",
    CurrentValue = false,
    Callback = function(ASQ)
        _G.quake1 = ASQ
    end    
})

spawn(function()
    while wait(getgenv().spamtime) do
        pcall(function()
            if _G.quake1 then 
                local player = game.Players.LocalPlayer
                local Mouse = player:GetMouse()

                local args1 = {
                    [1] = serializeTable(remotes),
                    [2] = "QuakePower4",
                    [3] = "StartCharging",
                    [5] = "Right"
                }
                
                player.Character.Powers.Quake.RemoteEvent:FireServer(unpack(args1))

                local args2 = {
                    [1] = serializeTable(remotes),
                    [2] = "QuakePower4",
                    [3] = "StopCharging",
                    [4] = Mouse.Target,
                    [5] = Mouse.Hit,
                    [6] = 100,
                    [7] = Vector3.new(player.Character.HumanoidRootPart.Position.X, player.Character.HumanoidRootPart.Position.Y, player.Character.HumanoidRootPart.Position.Z)
                }
                
                player.Character.Powers.Quake.RemoteEvent:FireServer(unpack(args2))
            end
        end)
    end
end)

-- Light Spam
local Toggle = SkillTab:CreateToggle({
    Name = "Light Spam | Light Beam |",
    CurrentValue = false,
    Callback = function(ASL)
        _G.lightbeam = ASL
    end    
})

spawn(function() 
    while wait(getgenv().spamtime) do
        pcall(function()
            if _G.lightbeam then
                local player = game.Players.LocalPlayer
                local Mouse = player:GetMouse()
                local humanoidRoot = player.Character.HumanoidRootPart
                local position = humanoidRoot.Position

                local args1 = {
                    [1] = serializeTable(remotes),
                    [2] = "LightPower2",
                    [3] = "StartCharging",
                    [4] = CFrame.new(position.X, position.Y, position.Z),
                    [5] = workspace:WaitForChild("IslandWindmill"):WaitForChild("Beach"):WaitForChild("Beach"),
                    [9] = "Left"
                }
                
                player.Character.Powers.Light.RemoteEvent:FireServer(unpack(args1))
                
                wait(0.05)
                
                local args2 = {
                    [1] = serializeTable(remotes),
                    [2] = "LightPower2",
                    [3] = "StopCharging",
                    [4] = Mouse.Hit,
                    [5] = workspace:WaitForChild("IslandWindmill"):WaitForChild("Beach"):WaitForChild("Beach"),
                    [6] = 100
                }
                
                player.Character.Powers.Light.RemoteEvent:FireServer(unpack(args2))
            end
        end)
    end
end)

