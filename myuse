                    [5] = workspace:WaitForChild("IslandTown"):WaitForChild("Beach"):WaitForChild("Beach"),
                    [7] = "Right"
                }

                game:GetService("Players").LocalPlayer.Character.Powers.Magma.RemoteEvent:FireServer(unpack(args1))
                wait()

                -- Segundo conjunto de argumentos
                local args2 = {
                    [1] = tonumber(serializeTable(remotes)),
                    [2] = "MagmaPower1",
                    [3] = "StopCharging",
                    [4] = CFrame.new(-1084.59033, -9763.44336, 501.375885, -0, 0.995015204, 0.0997241139, 0.0880244896, -0.099337019, 0.991152883, 0.996118307, 0.00877816416, -0.08758571),
                    [5] = workspace:WaitForChild("IslandTown"):WaitForChild("Beach"):WaitForChild("Beach"),
                    [6] = 100
                }

                game:GetService("Players").LocalPlayer.Character.Powers.Magma.RemoteEvent:FireServer(unpack(args2))
                wait(0.1)
            end)
        end
    end
})

-- Slider para controlar a velocidade do Yoru
