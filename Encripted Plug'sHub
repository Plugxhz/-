    Options.ToggleFastAttack:SetValue(true)

spawn(function()
	while wait(0.4) do
		pcall(function()
			if _G.FastAttackFaiFao then
				repeat wait(_G.Fast_Delay)
					AttackNoCD()
				until not _G.FastAttackFaiFao
			end
		end)
	end
end)

