    local ToggleRaceV2 = Tabs.Main:AddToggle("ToggleRaceV2", {Title = "Auto Evo Race V2", Description = "t·ª± ƒë√¥ng n√¢ng t·ªôc (ƒê√©o ph·∫£i n√¢ng v4 fully)", Default = false})
