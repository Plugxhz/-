local ToggleAutotrialafterkill = Tabs.Race:AddToggle("ToggleAutotrialafterkill", {Title = "kill player after trial", Description = "ƒë√°nh ng∆∞·ªùi ch∆°i ho√†n th√†nh trial", Default = false })
